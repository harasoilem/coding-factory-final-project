{
  "name": "nurse-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200 --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:dev": "ng serve --port 4200 --host 0.0.0.0 --disable-host-check",
    "start:80": "ng serve --port 80 --host 0.0.0.0 --disable-host-check",
    "build:other": "ng build --output-path=public --configuration production",
    "build:prod": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --configuration production --aot --output-hashing=all --output-path=public",
    "update:packages": "rm -rf node_modules && npm update --save-dev && npm update --save",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "django:migrate": "cd ../backend && pipenv run python manage.py migrate",
    "django:flush_db": "cd ../backend && pipenv run python manage.py flush --no-input",
    "django:init_test_data": "cd ../backend && pipenv run python manage.py init_test_data",
    "django:init_test_db": "cd ../backend && pipenv run python manage.py init_test_db",
    "django:runserver:dev": "cd ../backend && pipenv run python manage.py runserver 0.0.0.0:8000",
    "start:django:dev": "npm run django:migrate && npm run django:flush_db && npm run django:init_test_data && npm run django:runserver:dev",
    "setup:django:dev:env": "npm run django:migrate && npm run django:flush_db && npm run django:init_test_data",
    "cy:run": "npx cypress run --browser chrome",
    "cy:run:record": "npx cypress run --browser chrome --record --key cypress_key --parallel",
    "cy:run:record:dev": "npx cypress run --browser chrome --record --key cypress_key",
    "cy:ci": "IS_INTEGRATION_TESTING=1 start-test start:django:dev 8000 start:dev 4200 cy:run:record",
    "cy:ci:dev": "IS_INTEGRATION_TESTING=1 start-test start:django:dev 8000 start:dev 4200 cy:run:record:dev"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.9",
    "@angular/material-moment-adapter": "^15.2.9",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@auth0/angular-jwt": "^5.1.2",
    "@fortawesome/angular-fontawesome": "^0.12.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "lodash": "^4.17.20",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular/cli": "~15.2.6",
    "@angular/compiler-cli": "^15.2.0",
    "@faker-js/faker": "^8.0.2",
    "@types/jasmine": "~4.3.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.14.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "~4.9.4"
  }
}
